{
    "name": "k2oumais/lei",
    "description": "LEI generator and validator",
    "keywords": [
        "K2ouMais",
        "laravel",
        "lei"
    ],
    "homepage": "https://github.com/k2oumais/lei",
    "license": "MIT",
    "authors": [
        {
            "name": "Jo√£o Alves",
            "email": "joao.bastos.alves@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "ext-bcmath": "*",
        "spatie/laravel-package-tools": "^1.14.2",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "nunomaduro/collision": "^7",
        "orchestra/testbench": "^8.0.10",
        "pestphp/pest": "^2.0.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "K2ouMais\\Lei\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "K2ouMais\\Lei\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "K2ouMais\\Lei\\LeiServiceProvider"
            ],
            "aliases": {
                "Lei": "K2ouMais\\Lei\\Facades\\Lei"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
